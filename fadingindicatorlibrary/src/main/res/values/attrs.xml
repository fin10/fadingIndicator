<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="FadingIndicator">

        <attr
            name="radius"
            format="dimension"
            />

        <attr
            name="fillColor"
            format="color"
            />

        <attr
            name="strokeColor"
            format="color"
            />

        <attr name="shape" format="enum">
            <enum name="Circle" value="0"/>
            <enum name="Rectangle" value="1"/>
            <enum name="Triangle" value="2"/>
            <enum name="InvertedTriangle" value="3"/>
        </attr>

    </declare-styleable>



</resources>